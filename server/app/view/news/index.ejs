<%- include('/commom/header',{
  site_title:newsDetail?newsDetail.data[0].site_title:"11海南昆仑体育文化股份有限公司",
  Keywords:newsDetail?newsDetail.data[0].keywords:"海南昆仑体育文化股份有限公司",
  description:newsDetail?newsDetail.data[0].description:"海南昆仑体育文化股份有限公司",
}) %>
<%- include('/commom/banner') %>

<div class="content-full">
  <div class="content main-info">

    <%- include('/commom/column') %>
    <div class="content-right">
      <div class="news-list">
        <ul>
          <% newsDetail.data.forEach(item=>{ %>
          <li>
            <img src="" alt="" class="news-list-img" data-src="<%= item.file_path %>">
            <div class="news-list-right">
              <em><%= moment(item.created_at).format('YYYY-MM-DD') %></em>
              <h5><a href="<%= urlInfo +'/id/'+ item.id %>"><%= item.title %></a></h5>
              <p><%= item.description %></p>
            </div>
          </li>
          <% }) %>
        </ul>
      </div>
    </div>
    <%- include('/commom/page',{ url }) %>
  </div>
</div>
<%- include('/commom/serInfo') %>

<%- include('/commom/footer') %>