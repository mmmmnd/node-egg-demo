<%- include('/commom/header',{
  site_title:cultureList?cultureList.data[0].site_title:"11海南昆仑体育文化股份有限公司",
  Keywords:cultureList?cultureList.data[0].keywords:"海南昆仑体育文化股份有限公司",
  description:cultureList?cultureList.data[0].description:"海南昆仑体育文化股份有限公司",
}) %>
<%- include('/commom/banner') %>

<div class="content-full">
  <div class="content main-info">
    <%- include('/commom/column') %>
    <div class="content-right">
      <div class="news-list">
        <ul>
          <% cultureList.data.forEach(item=>{ %>
          <a href="<%= urlInfo +'/id/'+ item.id %>">
            <li>
              <img src="" alt="" class="news-list-img" data-src="<%= item.image %>">
              <div class="news-list-right">
                <em><%= moment(item.creatsed_at).format('YYYY-MM-DD') %></em>
                <h5><%= item.title %></h5>
                <p><%= item.description %></p>
              </div>
            </li>
          </a>
          <% }) %>
        </ul>
      </div>
    </div>
    <%- include('/commom/page',{ url }) %>
  </div>
</div>
<%- include('/commom/serInfo') %>

<%- include('/commom/footer') %>